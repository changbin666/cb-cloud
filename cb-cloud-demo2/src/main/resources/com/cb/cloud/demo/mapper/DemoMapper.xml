<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cb.cloud.demo.dao.DemoDao">
	<!-- 根据主键查询-->
	<select id="getById" resultType="com.cb.cloud.demo.entity.DemoEntity" parameterType="java.lang.Integer" >
        select  *
        from demo
        where id = #{id}
    </select>
</mapper>